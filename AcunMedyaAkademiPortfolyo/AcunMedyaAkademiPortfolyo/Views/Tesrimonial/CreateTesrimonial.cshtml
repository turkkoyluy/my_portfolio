@model AcunMedyaAkademiPortfolyo.Models.TblTesrimonial
@{
    ViewBag.Title = "CreateTesrimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Refarans Ekleme Sayfası</h2>
<form method="post">

    <label>Başlık</label>
    <input type="text" class="table table-bordered" name="Title" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="table table-bordered" name="Description" />
    <br />
    <label>Referans Adı</label>
    <input type="text" class="table table-bordered" name="NameSurname" />
    <br />
    <label>Resim Yolu</label>
    <input type="text" class="table table-bordered" name="ImageUrl" />
    <br />
    <label>Meslek</label>
    <input type="text" class="table table-bordered" name="Job" />
    <br />
    <label>Statü</label>
    <input type="checkbox" id="Status" name="Status" value="true" />
    <button class="btn btn-info">Refarans Kaydet</button>
</form>

